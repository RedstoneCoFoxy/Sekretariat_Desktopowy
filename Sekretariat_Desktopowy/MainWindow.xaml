                            <Button Visibility="Hidden" x:Name="Nauczyciel_ZmienPrawo" Margin="0,0,411,14" Height="40" Width="40" Background="#FFEFE9FF" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="Uczen_StworzRekord_Click_1">
