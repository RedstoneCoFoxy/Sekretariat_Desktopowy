                            <MenuItem.Header>
                                Plik
                            </MenuItem.Header>
                            <MenuItem x:Name="Menu_Plik_Wczytaj" Click="Plik_Wczytaj_Click" Background="#FFEFE9FF">
                                <MenuItem.Header>
                                    Wczytaj
                                </MenuItem.Header>
                            </MenuItem>
                            <MenuItem x:Name="Menu_Plik_Zapisz" Click="Plik_Zapisz_Click" Background="#FFEFE9FF">
                                <MenuItem.Header>
                                    Zapisz
                                </MenuItem.Header>
                            </MenuItem>
                            <MenuItem x:Name="Menu_Plik_Sortuj" Click="Plik_Sortuj_Click" Background="#FFEFE9FF">
                                <MenuItem.Header>
                                    Sortuj
                                </MenuItem.Header>
                            </MenuItem>
                            <MenuItem x:Name="Menu_Plik_Szukaj" Click="Plik_Szukaj_Click" Background="#FFEFE9FF">
                                <MenuItem.Header>
                                    Szukaj
                                </MenuItem.Header>
                            </MenuItem>
                        </MenuItem>
                    </Menu>
                    <ScrollViewer VerticalAlignment="Bottom" HorizontalAlignment="Left" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Visible" Height="354" Width="1010" >
