                            <Button x:Name="Uczen_ZapiszRekord" Margin="0,0,60,25" Height="25" Width="160" Background="#FFEFE9FF" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="Uczen_ZapiszRekord_Click" Visibility="Hidden" Grid.Column="1">
