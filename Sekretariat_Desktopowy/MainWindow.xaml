                            <Button x:Name="Nauczyciel_StworzRekord" Margin="0,0,60,25" Height="25" Width="160" Background="#FFEFE9FF" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="Nauczyciel_StworzRekord_Click">
                                Stwórz
                            </Button>
                            <Button x:Name="Nauczyciel_ZapiszRekord" Margin="0,0,60,25" Height="25" Width="160" Background="#FFEFE9FF" VerticalAlignment="Bottom" HorizontalAlignment="Right" Click="Uczen_StworzRekord_Click_1" Visibility="Hidden">
                                Zapisz
                            </Button>
                            <RadioButton x:Name="Nauczyciel_RadioTworz" GroupName="Nauczyicel_EdycjaRadio"  Height="36" Width="170" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="16" Margin="621,305,0,0" Content="Twórz Rekord" Checked="Nauczyciel_RadioTworz_Checked"/>
                            <RadioButton x:Name="Nauczyicel_RadioZapisuj" GroupName="Nauczyicel_EdycjaRadio"  Height="36" Width="170" VerticalAlignment="Top" HorizontalAlignment="Left" FontSize="16" Margin="621,325,0,-6" Content="Edytuj Rekord" Checked="Nauczyciel_RadioZapisuj_Checked"/>

